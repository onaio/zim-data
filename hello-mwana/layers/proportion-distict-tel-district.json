{
    "label": "Proportion of VHWs who submitted reports",
    "source": {
        "type": "vector",
        "data": {
            "type": "superset",
            "slice-id": 73},
        "layer": "zwe_adm2_district",
        "url": "mapbox://ona.62o31uy4",
        "join": [
            "ADM2_PCODE",
            "District_PCODE"
    ]
    },
    "type": "fill",
    "property": "tel_count",
    "categories": {
        "breaks": "no", 
        "limit": [50,79,1000],
        "label":["=<50%","51%-79%","=>80%"],
        "color": ["#FF0000","#f8bf84"," #008000"]
    },
    "aggregate": {
        "no-sort": true,
        "timeseries": {
            "type": "cumulative",
            "field": "week_year"
        }
    },
    
    "popup": {
        "header": "District",
        "body": " <p style=text-align:left;><b>Proportion:</b> {{tel_count}}</p>"
    },
    "visible": false,
    "credit": " "
}