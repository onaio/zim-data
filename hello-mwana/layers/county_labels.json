{
    "label": "County Labels",
    "source": {
        "type": "geojson",
        "data": "data/county-labels.geojson",
        "minzoom": 1
    },
    "type": "symbol",
    "minzoom": 0,
    "paint": {
        "text-color": "#000",
        "text-halo-color": "#fff",
        "text-halo-width": 1.3,
        "text-halo-blur": 1
    },
    "layout": {
        "text-size": 14,
        "text-field": "{county}",
        "text-offset": [
            0,
            0
        ],
        "text-allow-overlap": false
    },
    "visible": false
}