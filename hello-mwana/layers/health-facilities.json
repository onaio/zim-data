{
    "label": "Health Facilities",
    "source": {
        "type": "geojson",
        "featureType": "Point",
        "data": "data/health-facilities.csv",
        "join": [
            "Tel",
            "Tel"
              ]
    },
    "geo-columns": [
        "Long",
        "Lat"
    ],
    "type": "symbol",
    "paint": {
        "text-color": "#000",
        "text-halo-color": "#fff",
        "text-halo-width": 1.3,
        "text-halo-blur": 1
    },
    "layout": {
        "text-size": 14,
        "text-field": "",
        "text-offset": [
            0,
            1
        ],
        "icon-image": "hospital-15-black",
        "icon-allow-overlap": true
    },
    "categories": {
        "breaks": "no",
        "label": ["Health Facility"],
        "color": ["#000000"]
    },
    "popup": {
        "body": "Health Facility: {{Facility_Name}}"
    
    },
    "visible": false,
    "credit": "<i>Zoom in to see more health facilities</i> ",
    "category": "Settlements + Services"
}