{
    "label": "Proportion of girls with SAM",
    "source": {
        "type": "vector",
        "data":  {
            "type": "superset",
            "slice-id": 381},
            "layer": "zwe_adm2_district",
            "url": "mapbox://ona.62o31uy4",
            "join": [
                "ADM2_PCODE",
                "district_id"
            ]
    },
    "type": "fill",
    "property": "prop_girls_sam",
    "categories": {
        "breaks": "no", 
        "limit": [0.5,1.25,1000],
        "label":["=<0.5%","0.6%-1.25%","=>1.26%"],
        "color": ["#008000","#f8bf84","#FF0000"]
    },
    "aggregate": {
        "no-sort": true,
        "timeseries": {
            "type": "cumulative",
            "field": "period"
        },
        "filter": [
            "province","district"
        ],
        "accepted-filter-values": [
            "all","all"
        ],
        "filter-label": [
            "Province", "District"
        ],
        "filter-type": [
            "vBool","vBool"
        ]
    },
    "data-parse": {
        "district": {
            "type": "multiple",
            "split": ","
        },
        "province": {
            "type": "multiple",
            "split": ","
        }

    },
    
    "popup": {
        "header": "district",
        "body": " <p style=text-align:left;><b>Proportion:</b> {{prop_girls_sam}}%</p>"
    },
    "visible": false,
    "credit": " "
}