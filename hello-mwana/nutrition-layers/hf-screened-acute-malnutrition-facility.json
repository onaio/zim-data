{
    "label": "Number of children screened for acute malnutrition",
    "source": {
        "type": "geojson",
        "featureType": "Point",
        "data": {
         "type": "superset",
         "slice-id": 382},
        "join": [
         "id",
         "id"
 
        ],
        "minzoom": 3
         },
         "type": "circle",
        "property": "children_acute_mal",
        "radius-range": ["9","12","15"],
        "categories": {
          "breaks": "no", 
          "limit": [7,20,500000],
          "label":["0-7","8-20","20 or more"],
          "color": ["#FF0000","#f8bf84","#008000"]                                                                            
          
        },
         "geo-columns": ["longitude","latitude"],
    "aggregate": {
        "no-sort": true,
        "timeseries": {
            "type": "cumulative",
            "field": "period"
        },
        "filter": [
            "province","district","Facility"
        ],
        "accepted-filter-values": [
            "all","all","all"
        ],
        "filter-label": [
            "Province", "District", "Health Facility"
        ],
        "filter-type": [
            "vBool","vBool","vBool"
        ]
    },
    "data-parse": {
        "district": {
            "type": "multiple",
            "split": ","
        },
        "province": {
            "type": "multiple",
            "split": ","
        },
        "health_facility": {
            "type": "multiple",
            "split": ","
        }

    },
    "popup": {
        "header": "Facility",
        "body": " <p style=text-align:left;><b>Total children screened:</b> {{children_acute_mal}}</p>"
    },
    "visible": false,
    "credit": " "
}