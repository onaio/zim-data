{
    "label": "No. of people reached",
    "source": {
        "type": "vector",
        "data": "data/4W_Matrix_april9.csv",
        "layer": "zwe_adm2_district",
        "url": "mapbox://ona.62o31uy4",
        "join": [
            "ADM2_EN",
            "district"
        ]
    },
    "type": "fill",
    "property": "reached_people_todate",
    "aggregate": {
        "type": "sum",
        "group-by": "district",
        "extraProps": [
            "reached_people_todate",
            "implementing_partner",
            "district"
        ],
        "filter": ["implementing_partner"],
        "accepted-filter-values": ["all"],
        "filter-label": ["Implementing Partner"],
        "filter-type": ["stops"]
    },
    "categories": {
        "breaks": "yes",
        "color": "Blues",
        "clusters": 5
    },
    "popup": {
        "header": "district",
        "body": "<p style=text-align:left;> <b>No of people reached:</b> {{reached_people_todate}}</p>"
      },
    "credit": "UNICEF",
    "visible": false
}